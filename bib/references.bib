@article{wangTranscriptomeAssociationIdentifies2017,
  title = {Transcriptome {{Association Identifies Regulators}} of {{Wheat Spike Architecture}}},
  volume = {175},
  copyright = {\textcopyright{} 2017 American Society of Plant Biologists. All Rights Reserved..},
  issn = {0032-0889, 1532-2548},
  doi = {10.1104/pp.17.00694},
  abstract = {The architecture of wheat (Triticum aestivum) inflorescence and its complexity is among the most important agronomic traits that influence yield. For example, wheat spikes vary considerably in the number of spikelets, which are specialized reproductive branches, and the number of florets, which are spikelet branches that produce seeds. The large and repetitive nature of the three homologous and highly similar subgenomes of wheat has impeded attempts at using genetic approaches to uncover beneficial alleles that can be utilized for yield improvement. Using a population-associative transcriptomic approach, we analyzed the transcriptomes of developing spikes in 90 wheat lines comprising 74 landrace and 16 elite varieties and correlated expression with variations in spike complexity traits. In combination with coexpression network analysis, we inferred the identities of genes related to spike complexity. Importantly, further experimental studies identified regulatory genes whose expression is associated with and influences spike complexity. The associative transcriptomic approach utilized in this study allows rapid identification of the genetic basis of important agronomic traits in crops with complex genomes.},
  language = {en},
  number = {2},
  journal = {Plant Physiology},
  author = {Wang, Yuange and Yu, Haopeng and Tian, Caihuan and Sajjad, Muhammad and Gao, Caixia and Tong, Yiping and Wang, Xiangfeng and Jiao, Yuling},
  month = oct,
  year = {2017},
  pages = {746-757},
  file = {/home/tom/Zotero/storage/GCZMY2I7/Wang et al. - 2017 - Transcriptome Association Identifies Regulators of.pdf;/home/tom/Zotero/storage/YCT9FVXV/746.html},
  pmid = {28807930}
}

@article{swanson-wagnerReshapingMaizeTranscriptome2012,
  title = {Reshaping of the Maize Transcriptome by Domestication},
  volume = {109},
  issn = {0027-8424, 1091-6490},
  doi = {10.1073/pnas.1201961109},
  abstract = {Through domestication, humans have substantially altered the morphology of Zea mays ssp. parviglumis (teosinte) into the currently recognizable maize. This system serves as a model for studying adaptation, genome evolution, and the genetics and evolution of complex traits. To examine how domestication has reshaped the transcriptome of maize seedlings, we used expression profiling of 18,242 genes for 38 diverse maize genotypes and 24 teosinte genotypes. We detected evidence for more than 600 genes having significantly different expression levels in maize compared with teosinte. Moreover, more than 1,100 genes showed significantly altered coexpression profiles, reflective of substantial rewiring of the transcriptome since domestication. The genes with altered expression show a significant enrichment for genes previously identified through population genetic analyses as likely targets of selection during maize domestication and improvement; 46 genes previously identified as putative targets of selection also exhibit altered expression levels and coexpression relationships. We also identified 45 genes with altered, primarily higher, expression in inbred relative to outcrossed teosinte. These genes are enriched for functions related to biotic stress and may reflect responses to the effects of inbreeding. This study not only documents alterations in the maize transcriptome following domestication, identifying several genes that may have contributed to the evolution of maize, but highlights the complementary information that can be gained by combining gene expression with population genetic analyses.},
  language = {en},
  number = {29},
  journal = {Proceedings of the National Academy of Sciences},
  author = {{Swanson-Wagner}, Ruth and Briskine, Roman and Schaefer, Robert and Hufford, Matthew B. and {Ross-Ibarra}, Jeffrey and Myers, Chad L. and Tiffin, Peter and Springer, Nathan M.},
  month = jul,
  year = {2012},
  pages = {11878-11883},
  file = {/home/tom/Zotero/storage/PLQYUF2L/Swanson-Wagner et al. - 2012 - Reshaping of the maize transcriptome by domesticat.pdf;/home/tom/Zotero/storage/TVK52VR4/11878.html},
  pmid = {22753482}
}

@article{huangVariationFZPRegulatory,
  title = {Variation in {{{\emph{FZP}}}} Regulatory Region Causes Increases of Inflorescence Secondary Branches and Grain Yield in Rice Domestication},
  volume = {0},
  copyright = {This article is protected by copyright. All rights reserved.},
  issn = {1365-313X},
  doi = {10.1111/tpj.14062},
  abstract = {Inflorescence branching is a key agronomical trait determining rice yield. The ancestral wild rice (Oryza rufipogon Griff.) primary branch bears few grains due to minimal secondary branching. By contrast, O. sativa cultivars have been selected to produce large panicles with more secondary branches. Here we showed that the CONTROL OF SECONDARY BRANCH 1 (COS1) gene, which is identical to FRIZZY PANICLE (FZP), plays an important role in the key transition from few secondary branches of wild rice to more secondary branches in domesticated rice cultivars. A 4-bp tandem repeat deletion in 2.7-kb upstream of FZP may affect the binding activities of Auxin Response Factors (OsARFs) to the FZP promoter, decrease FZP expression level, and significantly enhance secondary branch number and grain yields in cultivated rice. Functional analyses showed that NARROW LEAF 1 (NAL1), a trypsin-like serine and cysteine protease, interacted with FZP and promoted FZP degradation. Consistently, down-regulating FZP expression or up-regulating NAL1 expression in commercial cultivar Zhonghua 17 increased secondary branches per panicle, grain number per panicle, and grain yield per plant. Our findings not only provide insights into the molecular mechanism of increasing grain number and yield during rice domestication, but also offer favorable genes for grain yield improvement of rice. This article is protected by copyright. All rights reserved.},
  language = {en},
  number = {ja},
  journal = {The Plant Journal},
  author = {Huang, Yongyu and Zhao, Shuangshuang and Fu, Yongcai and Sun, Hengdi and Ma, Xin and Tan, Lubin and Liu, Fengxia and Sun, Xianyou and Sun, Hongying and Gu, Ping and Xie, Daoxin and Sun, Chuanqing and Zhu, Zuofeng},
  keywords = {domestication,grain yield,inflorescence branching,protein degradation,rice (Oryza sativa)},
  file = {/home/tom/Zotero/storage/W3AMX94M/Huang et al. - Variation in FZP regulatory region causes increase.pdf;/home/tom/Zotero/storage/JS5ZB6QM/tpj.html}
}

