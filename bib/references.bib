  journal = {BMC Plant Biology},
  author = {{AL-Tam}, Faroq and Adam, Helene and dos Anjos, Ant\'onio and Lorieux, Mathias and Larmande, Pierre and Ghesqui\`ere, Alain and Jouannic, Stefan and Shahbazkia, Hamid Reza},
